{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Effect": "Allow",
      "Action": [
        "s3:GetObject",
        "s3:GetObjectVersion",
        "s3:PutObject",
        "s3:PutObjectAcl",
        "s3:GetBucketVersioning"
      ],
      "Resource": [
        "arn:aws:s3:::codepipeline-artifacts-akash-unique",
        "arn:aws:s3:::codepipeline-artifacts-akash-unique/*"
      ]
    }
  ]
}
